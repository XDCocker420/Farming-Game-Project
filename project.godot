; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Farming Game Project"
run/main_scene="res://scenes/maps/game_map.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
run/max_fps=60
config/icon="res://icon.svg"

[autoload]

SaveGame="*res://scripts/data/save_game.gd"
ConfigReader="*res://scripts/config/ConfigReader.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"marlene": "res://dialogs/characters/marlene.dch",
"martha": "res://dialogs/characters/martha.dch",
"theoden": "res://dialogs/characters/theoden.dch"
}
directories/dtl_directory={
"intro_timeline": "res://dialogs/story/timelines/intro_timeline.dtl",
"template timeline": "res://dialogs/story/timelines/template timeline.dtl",
"theoden_main": "res://dialogs/story/timelines/theoden_main.dtl",
"tutorial_timeline": "res://dialogs/story/timelines/tutorial_timeline.dtl"
}
glossary/default_case_sensitive=true
layout/style_list=["res://dialogs/story/visual_novel_style.tres", "res://dialogs/story/speaker_style.tres", "res://dialogs/story/dialog_style.tres"]
layout/default_style="res://dialogs/story/visual_novel_style.tres"
variables={
"global": {
"player_name": "",
"tutorial": false
}
}
extensions_folder="res://addons/dialogic_additions"
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
audio/max_channels=4.0
animations/join_default_length=0.5
animations/join_default_wait=true
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/cross_fade_default_length=0.5
choices/autofocus_first=false
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=true
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
save/autosave=true
glossary/glossary_files=["res://dialogs/story/glossary/glos1.tres", "res://dialogs/story/glossary/characters.tres"]
translation/enabled=false
translation/original_locale="de"
translation/file_mode=1
translation/translation_folder="res://translations"
translation/save_mode=0
translation/add_separator=false
history/simple_history_enabled=true
history/full_history_enabled=false
choices/hotkey_behaviour=1

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="viewport"

[dotnet]

project/assembly_name="Project Farming Game"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg")

[global_group]

Player=""
Map=""
dynamic_elements="all dynamic elements that should be saved"

[gui]

theme/custom="res://default_theme.tres"

